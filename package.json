{
  "name": "cobo-home-work",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "server": "npx tsc -p server/tsconfig.json && node server/dist/index.js",
    "client": "npm run dev",
    "dev:both": "concurrently \"npm run server\" \"npm run client\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --config jest.config.js",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch --config jest.config.js",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.92.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/koa": "^3.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "jest-transform-css": "^6.0.1",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "react-testing-library": "^8.0.1",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
